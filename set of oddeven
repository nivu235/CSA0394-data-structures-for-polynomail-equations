#include<stdio.h>
int main()
{
	int a[10],i,n;
	printf("enter the n value");
	scanf("%d",&n);
	printf("enter the number");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("%d is a even number \n",a[i]);
		}
		else
		{
			printf("%d is a odd number \n ",a[i]);
		}
	}
}
